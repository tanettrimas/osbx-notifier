{
  "name": "osbx-notifier",
  "version": "1.0.2",
  "description": "Notification service for new companies on OSBX",
  "main": "dist/index.js",
  "scripts": {
    "compile": "rimraf ./dist && tsc",
    "prestart": "npm run compile",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "test": "jest",
    "pretest": "npm run lint",
    "lint:fix": "eslint --ext .js,.ts --fix .",
    "lint": "eslint --ext .js,.ts .",
    "typecheck": "tsc -p .",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanettrimas/osbx-notifier.git"
  },
  "keywords": [
    "notification",
    "sms",
    "stocks",
    "shares",
    "osbx"
  ],
  "author": "Tanet Trimas <tanet@trimas.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanettrimas/osbx-notifier/issues"
  },
  "homepage": "https://github.com/tanettrimas/osbx-notifier#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.10",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  }
}
